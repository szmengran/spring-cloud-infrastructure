				<artifactId>druid-spring-boot-starter</artifactId>
				<version>1.1.10</version>
			</dependency>
			<dependency>
				<groupId>com.szmengran</groupId>
				<artifactId>orm-mysql</artifactId>
				<version>${orm.version}</version>
			</dependency>
			<dependency>
				<groupId>com.szmengran</groupId>
				<artifactId>orm-oracle</artifactId>
				<version>${orm.version}</version>
			</dependency>
			<dependency>
				<groupId>com.suntak</groupId>
				<artifactId>aop-exception</artifactId>
				<version>${aop.version}</version>
			</dependency>
			<dependency>
				<groupId>com.suntak</groupId>
				<artifactId>aop-logging-service</artifactId>
				<version>${aop.version}</version>
			</dependency>
			<dependency>
				<groupId>com.google.code.gson</groupId>
